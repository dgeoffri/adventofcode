#!/usr/bin/python
# coding: utf-8

def lookandsay(number):
	temp = list()
	for i in str(number):
		if len(temp) > 0 and int(i) == temp[-1][1]:
			temp[-1][0] += 1
		else:
			temp.append([1,int(i)])
	return ''.join([''.join(map(str,i)) for i in temp])

num = 311332213

for _ in xrange(39):
	num = lookandsay(num)

print len(num)
